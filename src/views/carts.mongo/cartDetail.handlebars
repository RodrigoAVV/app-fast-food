<section class="row row-cols-1 row-cols-md-4 g-4">
    {{#each products}}
        <article class="col">
            <div class="card h-100">
                <img src={{this.product.thumbnail}} alt="" class="card-img-top">
                <div class="card-body">
                    <p class="card-title">Nombre: {{this.product.title}}</p>
                    <p class="card-text">Cantidad: {{this.quantity}}</p>
                    <button type="button" class="btn btn-primary deleteProduct">Quitar producto</button>
                </div>
            </div>
        </article>
    {{/each}}
</section>
