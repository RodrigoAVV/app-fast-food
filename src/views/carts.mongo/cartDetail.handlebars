<section class="row row-cols-1 row-cols-md-4 g-4">
    {{#each products}}
        <article class="col">
            <div class="card h-100">
                <img src={{this.product.thumbnail}} alt="" class="card-img-top">
                <div class="card-body">
                    <p class="card-title">{{this.product.title}}</p>
                    <p class="card-text">{{this.quantity}}</p>
                    <a href="#" class="btn btn-primary">Quitar del carrito</a>
                </div>
            </div>
        </article>
    {{/each}}
</section>
